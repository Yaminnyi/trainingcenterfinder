<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <title>STCW courses</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <style>
    	.course{
    		width:200px;   
    		border:1px solid black;
        background-color: blue;
         		
    	}
    	
    </style>
 </head>

  <body>
  <h1>STCW courses</h1>
  <p><a href="../cart">View cart</a> | <a href="../emptycart">Empty cart</a></p>


  <% data.forEach(function(course){ %>

  
    <div class="course">

      <h3><%= course.name %></h3>
      <p><%= course.course %><p>
      <p><%= course.date %><p>
      <p><%= course.detail %><p>
      <p><%= course.end %><p>
      <p><%= course.address %><p>
      <p>price: <%= course.price %></p>


	    
  	</div>

     
  <% }); %>
  
  

  

 </body>

</html>